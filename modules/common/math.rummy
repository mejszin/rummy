3.14159265359 pi alias

inc proc
    1 +
    return
end

dec proc
    1 -
    return
end

floor proc
    dup 1 % -
    return
end

ceil proc
    dup 1 % 1 swap - +
    return
end

sum proc
    dup 1 equals unless
        rotate! !1 !subtract
        add !rotate
        sum
    else
        drop!
    end
    return
end