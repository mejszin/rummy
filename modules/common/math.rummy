3.14159265359 pi alias

return

inc:
    1 +
    return

dec:
    1 -
    return

floor:
    dup 1 % -
    return

ceil:
    dup 1 % 1 swap - +
    return

sum:
    rotate
    sum_until_zero:
        !1 !subtract
        !dup !0 !equals
        !sum_finish !gotoif
        add
        sum_until_zero goto
    sum_finish:
        !drop
        return