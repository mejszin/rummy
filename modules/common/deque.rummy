# Drops N items from opposite direction

drops proc
    dup! 0! equals! unless
        1! subtract!
        !drop drops!
    else
        drop!
    end
    return
end

rotates proc
    dup 0 = unless
        1 -
        swap rotate!
        rotates
    else
        drop!
    end
    return
end

pair proc
    rotate! dup! !rotate dup! rotate! swap! !rotate
    return
end

